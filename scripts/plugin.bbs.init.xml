<script>
<name>plugin.bbs.init</name>
<version>1</version>
<engineversion>72</engineversion>
<description>Guilds: Init BBS articles</description>
<arguments>
<argument index="1" name="guilds.data" type="Value" desc="Data" />
</arguments>
<sourceplaintext>
<![CDATA[
  * add bbs articles
  $tArticles = table alloc
  $guild.data['bbs.articles'] = $tArticles
  
  $race = null
  $article = 'g'
  $page = 2032
  gosub sub.CreateBBSAll
  
  $race = Argon
  $article = 'argon'
  $page = 2033
  gosub sub.CreateBBSAll
  
  $race = Boron
  $article = 'boron'
  $page = 2024
  gosub sub.CreateBBSAll
  
  $race = Split
  $article = 'split'
  $page = 2035
  gosub sub.CreateBBSAll
  
  $article = 'paranid'
  $page = 2036
  $race = Paranid
  gosub sub.CreateBBSAll
  
  $article = 'teladi'
  $page = 2037
  $race = Teladi
  gosub sub.CreateBBSAll
  
  $article = 'pirate'
  $race = Pirates
  $page = 2038
  gosub sub.CreateBBSAll
  
  $article = 'goner'
  $page = 2039
  gosub sub.CreateBBSAll
  
  * =====================
  return null
  
  
  sub.CreateBBSAll:
  $id = 1
  while $id < 70
    gosub sub.CreateBBSParsed
    inc $id = 
  end
@ = wait 1 ms
  endsub
  
  sub.CreateBBSParsed:
  if read text: page id=$page, id=$id exists
    $article.text = sprintf: fmt='%s.%s', $article, $id, null, null, null
    $table = table alloc
    $tArticles[$article.text] = $table
    $table['is.dynamic'] = [FALSE]
    $table['article'] = $article.text
    $table['id'] = $id
    $table['page.id'] = $page
    if $race
      $table[$race] = $race
    end
    
    $value = 10000 + $id
    if read text: page id=$page, id=$value exists
      $table['tab'] = $value
    else
      $read.text = read text: page=$page id=$id
      $idx = find position of pattern '[title]' in $read.text
      if $idx != -1
        $idx = $idx + 7
        $endIdx = find position of pattern '[/title]' in $read.text
        if $endIdx != -1
          if $endIdx > $idx
            $len = $endIdx - $idx
            $read.text = get substring of $read.text offset=$idx length=$len
            $table['tab'] = $read.text
          end
        end
      end
    end
  end
  endsub
  
  
  
  sub.CreateBBS:
  $article.text = sprintf: fmt='%s.%s', $article, $id, null, null, null
  $table = table alloc
  $tArticles[$article.text] = $table
  $table['is.dynamic'] = [FALSE]
  $table['article'] = $article.text
  $table['id'] = $id
  $value = $id + 1
  $table['author'] = $value
  $value = $id + 2
  $table['tab'] = $value
  $value = $id + 3
  $table['title'] = $value
  $table['page.id'] = $page
  endsub
  
  
  return null
]]>
</sourceplaintext>
