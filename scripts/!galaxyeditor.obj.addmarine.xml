<script>
<name>!galaxyeditor.obj.addmarine</name>
<version>1</version>
<engineversion>73</engineversion>
<description>Description</description>
<arguments>
<argument index="1" name="amount" type="Var/Number" desc="amount" />
<argument index="2" name="exp" type="Var/Boolean" desc="Experienced Marines?" />
<argument index="3" name="probabilities" type="Var/Table" desc="" />
</arguments>
<sourceplaintext>
<![CDATA[
  $debug = [FALSE]
  
  skip if [THIS] -> get true amount of ware Cargo Lifesupport System in cargo bay
    = [THIS] -> install 1 units of Cargo Lifesupport System
  
  skip if $probabilities
@   $probabilities = [THIS] -> call script lib.guilds.marine.getraceprobability :  =[OWNER]
  
  Skip.Noto.Prob.Calcs:
  
  * Now actually generate the marines
  while $amount
    dec $amount = 
    gosub sub.PickRace
    $name = get random name: race=$race
    $face = random value from 0 to 4 - 1
    $voice = random value from 0 to 4 - 1
    $passenger = [THIS] -> create passenger in ship: name=$name race=$race voice=$voice face=$face
    skip if $passenger -> exists
      continue
    if $exp
      $fight = random value from 50 to 101 - 1
      $hacking = random value from 50 to 101 - 1
      $mechanical = random value from 50 to 101 - 1
      $engineering = random value from 50 to 101 - 1
      $passenger ->create marine from passenger: fighting=$fight hacking=$hacking mechanical=$mechanical engineering=$engineering
    else
      $passenger ->train passenger to marine
    end
  end
  
  return null
  
  * ===============
  * Select a race to use for marine. 
  * ===============
  sub.PickRace:
  $rand = random value from 1 to 1001 - 1
  $probabilityOld = 0
  $key = get next key: table=$probabilities key=null
  while $key
    $probability = $probabilities[$key]['probability']
    $probability = $probability + $probabilityOld
    if $probability > $rand
      $races = $probabilities[$key]['races']
      $race = get random array item: array=$races
      break
    end
    $probabilityOld = $probability
    $key = get next key: table=$probabilities key=$key
  end
  do if $debug
    write to log file #$debug  append=[TRUE]  printf: fmt='Rand was %s, race was %s', $rand, $race, null, null, null
  endsub
  
  return null
]]>
</sourceplaintext>
